{
    "forceSingleLine" :["dataController","form","div","group","vgroup","hgroup","verticalgroup","horizontalgroup"],
    "minAttrForMultiLine": 4,
    "minAttrLengthForMultiLine":150,
    "defaultAttrOrder":["id","dataBinding","label","labelStyle","name","size","disabled","readOnly","hidden","title","titlePosition","required","requiredMessage"],
    "defaultAttrTrailing":["onchange","onvalidate","onvalidateAsync",
                "style","enclosingClass","enclosingStyle","containerStyle","controlStyle"],
    "tagOptions":{
        "<default>":{
            "attrOrder":[]
        },
        "date":{
            "attrOrder":["format","separator","userCalendar","numberOfMonths"]
        },
        "time":{
            "attrOrder":["timeType","enableToggle","showSeconds","flagNoChangeEvent","ampmNames"]
        },
        "phone":{
            "attrOrder":["maxlength","buttonEnableMode","onclickButton","buttonVisibility"]
        },
        "integer":{
            "attrOrder":["max","min","maxlength","value","scale","step","prefix","suffix","numberAlign","useCalculator","useCalculatorWithHotKeys","calculatorOpen"]
        },
        "numeric":{
            "copyOf":"integer"
        },
        "SSN":{
            "attrOrder":["concealOnBlur","blurMask","focusMask"],
            "includeTagsAfter":"text"
        },
        "password":{
            "copyOf":"text"
        },
        "zipCode":{
            "copyOf":"text"
        },
        "text":{
            "attrOrder":["upperCase","maxlength","value","listTitle","buildRoutine","globalReference","pieceDescriptor","valueList","displayList","embeddedDisplayField","embeddedDisplayFieldClass","embeddedDisplayFieldSize",
                        "embeddedDisplayFieldStyle","buttonEnableMode","onclickButton","buttonVisibility"]
        },
        "findList":{
            "attrOrder":["upperCase","listTitle","buildRoutine","globalReference","pieceDescriptor","valueList","displayList","queryClass","queryName","onbuildList","valueColumn","displayColumn",
                "multipleValues","unrestricted","ondrillOver"],
            "includeTagsAfter":"text"

        },
        "valueList":{ 
            "attrOrder":["unrestricted"],
            "includeTagsAfter":"text"
        },
        "explorerList":{ 
            "attrOrder":["tableId","additionalTables","listType","placeholder","includeRParents","includeRChildren","includeHParent","includeHChildren","autojoin","joinTableExcludeTypes","includeTypes","excludeTypes","includeFunc","excludeKey",
                "excludeReadOnly","excludeReadOnlyTables","foreignExcludeTypes","isExpression","unrestricted","ondrillOver","onFormatListValue"],
            "includeTagsAfter":"text"
        },
        "expression":{
            "copyOf":"explorerList"
        },
        "modSecurity":{
            "copyOf":"radioSet"
        },
        "radioSet":{
            "attrOrder":["onbuildList","onbuildValueList","onbuildDisplayList","onbuildTitleList","valueList","displayList","titleList","layout","emptyCaption"]
        },
        "radioButton":{
            "attrOrder":["onbuildList","valueList","displayList","titleList","imageList","iconList","imageStyle","textStyle","textClass"]
        },
        "setData":{
            "attrOrder":["tableId","fieldListType","dataListType","quoteLiterals","allowExpressions","onbuildExplorerList"]
        },
        "selCriteria":{
            "attrOrder":["tableId","additionalTables","dataColAdditionalTable","dataColTableId","listType","dataColListType","listOptionsJSON","dataColListOptionsJSON","showUITab","showSQLTab","onbuildOperatorList","onvalidateDataFieldControl"]
        },
        "fileUpload":{
            "attrOrder":["caption","allowManualUpload","uploadType","validTypes","uploadService","onclearFile","onselectFile","onuploadComplete","onuploadFail"]
        },
        "accordionGroup":{
            "attrOrder":["expandMode","initiallyExpanded","toggleMode"]
        },
        "accordionPanel":{
            "attrOrder":["caption","image","canExpand","canCollapse"]
        },
        "checkBox":{
            "attrOrder":["allowIndeterminate"]
        },
        "button":{
            "attrOrder":["caption","captionStyle","captionClass","wrapCaption","captionPosition","image","imageStyle","imageClass","onclick","securityCodes","useRoundedCorners"]
        },
        "div":{
            "attrOrder":["class","style","layout","height","width"]
        },
        "textArea":{
            "attrOrder":["rows","cols","resize"],
            "includeTagsAfter":"text"
        },
        "grid":{
            "attrOrder":["caption","height","width","editMode",
                "canAdd","onrowAddition","canEdit","onrowEdit","canRemove","onrowRemoval","canPurge","onpurge",
                "ongetDataSource","reloadOnRefresh","valueColumn","readOnly","useDisabledAsReadOnly",
                "showHeader","canAggregate","canGroup","canSort","focusOnBindingComplete","loadOnRender","maxRows",
                "drillColumn","groupingColumns","groupsExpandedByDefault","onbindingComplete",
                "ondataLoaded","oncellRender","onready","onrefreshData","onreloadData","onrowSelect","onrowUnselect","onrowMoved","onrowClick","onrowDoubleClick","onrowDrill"]
        },
        "tableGrid":{
            "attrOrder":["caption","height","width","tableId","parentTableId"],
            "includeTagsAfter":"grid"
        },
        "queryGrid":{
            "attrOrder":["caption","height","width","tableId","queryClass","queryName","queryIncludeKeyCol"],
            "includeTagsAfter":"grid"
        },
        "globalGrid":{
            "attrOrder":["caption","height","width","buildRoutine","globalReference","pieceDescriptor"],
            "includeTagsAfter":"grid"
        },
        "gridParameter":{
            "attrOrder":["value"],
            "forceSingleLine":true
        },
        "gridColumn":{
            "forceSingleLine":true,
            "attrOrder":["colName","colHeader","unique","width","size","maxlength","canAggregate","canFilter","canGroup","canSort"]
        }

    },
    "tagOptionsByRegex":{
        "^grid.+":{ "forceSingleLine":true,"copyOf":"gridColumn"}
        
    }

}